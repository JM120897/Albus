<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record id="act_alquiler" model="ir.actions.act_window">
            <field name="name">Alquiler</field>
            <field name="res_model">alquiler</field>
            <field name="view_type">form</field>
            <field name="view_mode">form, tree</field>
        </record>
         <record id="view_alquiler_form" model="ir.ui.view">
            <field name="name">view_alquiler_form</field>
            <field name="model">alquiler</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="tree_string">
                    <field name="ID" />
                    <field name="Horas" />
                    <field name="Fecha" />
                    <field name="NumPersonas" />
                    <field name="Descripcion" />
                   	<field name="LugarOrigen" />
                    <field name="LugarDestino" />
                </form>
            </field>
        </record>
          <record id="view_alquiler_tree" model="ir.ui.view">
            <field name="name">view_alquiler_tree</field>
            <field name="model">alquiler</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="tree_string">
                   <field name="ID" />
                    <field name="Horas" />
                    <field name="Fecha" />
                    <field name="NumPersonas" />
                    <field name="Descripcion" />
                   	<field name="LugarOrigen" />
                    <field name="LugarDestino" />
                </tree>
            </field>
            </record>
        <menuitem id="alquiler_menu" name="Alquiler" action="act_alquiler" />
        <menuitem name="Alquiler" id="alquiler_submenu" parent="alquiler_menu" sequence="100" action="act_alquiler" />
    </data> 
</openerp>